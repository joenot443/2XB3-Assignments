	{
		return this.score;
	}
	
	public void setScore(int s)
	{
		this.score = s;
	}
	
	public String getWord()
	{

		return this.word;
	}
	
	public void setWord(String w)
	{
		this.word = w;
	}
	
	@Override
	public int compareTo(Word w)
	{
		//TODO
		return 0;
	}
	
	
	public String toString()
	{
		//TODO
		return "";
	}

}
